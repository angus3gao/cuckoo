<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                 http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<!-- 服务器配置信息 -->
	<bean id="serverConfig" class="com.angus.cuckoo.core.servevr.config.ServerConfig">
		<property name="serverType" value="master" />
		<property name="regionId" value="${config.regionId}" />
		<property name="serverId" value="${config.serverId}" />
		<property name="ip" value="${config.serverId}" />
		<property name="ports" value="${config.ports}" />
	</bean>
	
</beans>